#include <iostream>
using namespace std;

int main(){
    int n;
    int i;
    int count;
    cout<<"Enter a positive integer value: ";
    cin>> n;
      if(n==1){
        cout<<"Not a prime number";
      }
    for( int i=2; i*i< n; i++){
         if(n%i==0){
         	count++;
         }
    }
   if (count > 2)
    cout<<"Not prime";
    else
    cout<<"Prime";

}//end main
