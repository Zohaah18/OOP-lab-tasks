#include <iostream>
using namespace std;
void recursiveswap(int &a, int &b) {
    if (a > b) {
        swap(a, b);
    }
}

int main() {
    int x = 10;
    int y = 8;
    cout<<"Before swap x: "<< x <<" y: "<< y <<endl;
    recursiveswap(x, y);
    cout <<"After swap x:"<< x <<" y: "<< y <<endl; 
}
